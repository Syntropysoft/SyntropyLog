{
  "name": "syntropylog",
  "version": "0.3.0",
  "description": "A modern, composable observability framework for Node.js",
  "author": "Gabriel Alejandro Gomez",
  "license": "Apache-2.0",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "syntropylog": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./doctor": {
      "import": "./dist/doctor.mjs",
      "require": "./dist/doctor.cjs"
    },
    "./http": {
      "import": {
        "types": "./dist/http/index.d.ts",
        "default": "./dist/http/index.mjs"
      },
      "require": {
        "types": "./dist/http/index.d.ts",
        "default": "./dist/http/index.cjs"
      }
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && rollup -c",
    "dev": "rollup -c -w",
    "clean": "rimraf dist",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "vitest run",
    "check:deps": "depcheck",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Syntropysoft/beaconlog.git"
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "chalk": "^5.3.0",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "on-finished": "^2.4.1",
    "uuid": "^9.0.1",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@aws-sdk/client-kinesis": "^3.485.0",
    "@azure/event-hubs": "^5.11.0",
    "@azure/service-bus": "^7.9.1",
    "@google-cloud/pubsub": "^4.0.7",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/amqplib": "^0.10.4",
    "@types/async-retry": "^1.4.8",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.5",
    "@types/on-finished": "^2.3.4",
    "@types/uuid": "^9.0.7",
    "@types/yargs": "^17.0.32",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitest/coverage-v8": "^1.1.0",
    "amqplib": "^0.10.3",
    "axios": "^1.6.3",
    "depcheck": "^1.4.7",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "got": "^14.0.0",
    "kafkajs": "^2.2.4",
    "mqtt": "^5.3.4",
    "nock": "^13.4.0",
    "prettier": "^3.1.1",
    "redis": "^4.6.12",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.2",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "peerDependencies": {
    "@aws-sdk/client-kinesis": "^3.485.0",
    "@azure/event-hubs": "^5.11.0",
    "@azure/service-bus": "^7.9.1",
    "@google-cloud/pubsub": "^4.0.7",
    "amqplib": "^0.10.3",
    "axios": "^1.6.3",
    "got": "^14.0.0",
    "kafkajs": "^2.2.4",
    "mqtt": "^5.3.4",
    "redis": "^4.6.12"
  },
  "peerDependenciesMeta": {
